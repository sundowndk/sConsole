<sui elementheight="50px">
	<canvas width="800px" height="400px" canScroll="false">	
		<container title="Edit usergroup" icon="Icon32Edit" stylesheet="SUIContainerModal">
			<layoutbox type="horizontal" stylesheet="LayoutboxNoborder">			
				<panel size="*">
					<layoutbox type="horizontal">			
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Name"/>
								</panel>					
								<panel size="*">
									<textbox tag="name" width="100%" focus="true" />
								</panel>					
							</layoutbox>
						</panel>
						<panel size="*">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Members"/>
								</panel>					
								<panel size="*">
									<listview tag="users" width="100%" height="100%" disabled="true">
										<column tag="id" />						
										<column tag="username" label="Username" width="200px" visible="true" />
										<column tag="realname" label="Realname" width="200px" visible="true" />
									</listview>								
								</panel>					
							</layoutbox>
						</panel>						
					</layoutbox>
				</panel>
				<panel size="45px">
					<layoutbox type="vertical" stylesheet="LayoutboxNoborder">
						<panel size="*">								
						</panel>
						<panel size="210px">
							<button tag="save" label="Save" width="100px" disabled="true"/>
							<button tag="close" label="Close" width="100px" />
						</panel>						
					</layoutbox>
				</panel>
			</layoutbox>				
		</container>
	</canvas>
</sui>



