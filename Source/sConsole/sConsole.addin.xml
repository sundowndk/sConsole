<Addin id          = "sConsole"              
       author      = "Rasmus Pedersen"
       copyright   = "MIT X11"
       url         = "http://www.sundown.dk/"
       description = "Convenient UI to manage the Sorento Engine."	   
       version     = "2.0">
	<Runtime>
		<Import assembly="sConsole.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Sorento" version="1.0"/>
	</Dependencies>
	
	<Extension path = "/SorentoLib/Init">
		<Type type="sConsole.Addin.Init"/>
	</Extension>		
						
	<Extension path = "/SorentoLib/Render/Placeholders">
		<Type type="sConsole.Addin.Placeholder"/>
	</Extension>								
						
	<Extension path = "/SorentoLib/Resolver/Ajaxes">
		<Type type="sConsole.Addin.Ajax"/>
	</Extension>			
</Addin>