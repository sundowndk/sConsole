<sui elementheight="50px">	
	<layoutbox type="horizontal" stylesheet="LayoutboxNoborder">			
		<panel size="%elementheight%">
			<layoutbox type="vertical">
				<panel size="100px">
					<label text="Username"/>
				</panel>					
				<panel size="*">
					<textbox tag="username" width="100%" focus="true" />
				</panel>					
			</layoutbox>
		</panel>
		<panel size="%elementheight%">
			<layoutbox type="vertical">
				<panel size="100px">
					<label text="Email"/>
				</panel>					
				<panel size="*">
					<textbox tag="email" width="100%" />
				</panel>					
			</layoutbox>
		</panel>
	</layoutbox>
</sui>
