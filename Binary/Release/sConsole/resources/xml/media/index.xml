<sui>
	<container title="Media" icon="Icon32Edit" stylesheet="SUIContainerNoBorder">		
		<tabview>			
			<tab label="Transformations" selected="true">
				<layoutbox type="vertical" width="100%" height="100%" stylesheet="SUILayoutboxNoBorder">
					<panel size="*">
						<listview tag="mediatransformations" width="100%" height="100%">
							<column tag="id" />
							<column tag="title" label="Title" width="300px" visible="true" />
							<column tag="mimetypes" label="Mimetypes" width="200px" visible="true" />
						</listview>
					</panel>
					<panel size="100px">
						<button tag="mediatransformationcreate" width="100%" label="Create" />
						<button tag="mediatransformationedit" width="100%" label="Edit" />
						<button tag="mediatransformationdelete" width="100%" label="Delete" />
					</panel>
				</layoutbox>				
			</tab>
		</tabview>
	</container>	
</sui>
